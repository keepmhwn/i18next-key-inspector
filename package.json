{
  "name": "i18next-key-inspector",
  "version": "1.0.1",
  "description": "i18next-key-inspector is a tool that reads JSON-format translation files written for multilingual support to inspect if each locale has the same keys.",
  "main": "./dist/main.js",
  "type": "module",
  "scripts": {
    "start": "webpack --mode=development && node ./dist/main.js",
    "build": "webpack --mode=production",
    "lint": "eslint ./src"
  },
  "bin": {
    "inspect-locale-key": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "i18next-key-inspector",
    "i18next",
    "translation",
    "internationalization"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
